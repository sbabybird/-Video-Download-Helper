{
  "manifest_version": 3,
  "name": "Video Download Helper",
  "version": "0.1.0",
  "description": "Detects videos on a page and sends them to a native client for download.",
  "permissions": [
    "nativeMessaging",
    "scripting"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content_script.js"]
    }
  ],
  "icons": {}
}
